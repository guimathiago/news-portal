{
  "source": [
    "tokens/tokens.json"
  ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "transforms": [
        "attribute/cti",
        "name/cti/kebab",
        "color/hex",
        "size/px"
      ],
      "buildPath": "build/",
      "files": [
        {
          "destination": "scss/_variables.scss",
          "format": "scss/variables"
        }
      ]
    }
  }
}